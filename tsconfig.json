{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2019",
		"lib": ["ES2019"],
		"outDir": "out",
		"rootDir": "src",
		"sourceMap": true,
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"src/test",
		"node_modules",
		".vscode-test"
	],
	"references": []
}
